#include <stdio.h>

#include <stdbool.h>


int main ()
{

	int array[4]={1,2,3,4};
	
	
	for(int i=0;i<4;i++){
	    
	    
	    int*p=&array[i];
	    
	    
	    
	    printf("stamp l'indirizzo del [%d] = %p \n",i, p);// stampare tutti gli indirizzi dal primo indice fino all'ultimo del array 
	    
	    
	    
	    
	}




	return 0 ;
}
