#include <stdio.h>

#include <stdbool.h>


int main ()
{
                         
                       //prima colonna ,seconda colonna ,terza riga 
   int matrice [3][3]={{10,20,30}, //prima riga 
                       {30,10,20}, //seconda riga 
                       {40,20,30}};// terza riga 
                       
                       
                       
                       
                       
        for(int riga=0;riga<3;riga++){
            
            for(int colonna=0;colonna<3;colonna++){
                
                
                        //il primo numero in matrice      // numero di riga   // numero di colonna
                
                printf("|%d|  ",matrice[riga][colonna]);
              


                
               
                
                
            }
            
            printf("\n");
            
        }


return 0;
}
